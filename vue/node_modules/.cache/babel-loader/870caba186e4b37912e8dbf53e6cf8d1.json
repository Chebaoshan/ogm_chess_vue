{"ast":null,"code":"import { useGameStore } from '../store/gameStore';\nimport { winner } from '../utils/winner';\nexport default {\n  name: \"Square\",\n  props: ['position'],\n\n  setup() {\n    const store = useGameStore();\n    return {\n      store\n    };\n  },\n\n  data() {\n    return {\n      pos: this.position,\n      cell_value: null,\n      refresh: this.store.getResult()\n    };\n  },\n\n  watch: {\n    refresh() {}\n\n  },\n  methods: {\n    handleClick() {\n      const a = this.store.getCount() % 2 == 0 ? 'X' : 'O';\n\n      if (this.cell_value === null) {\n        this.cell_value = a;\n        this.store.renewHistory(this.pos);\n        this.store.addCount();\n      }\n\n      const res = winner(this.store.history);\n\n      if (res.result) {\n        alert(`${res.winner}`);\n        this.store.setResult();\n        this.store.$reset();\n      }\n    }\n\n  },\n  computed: {\n    getPlayer() {\n      return this.store.currentPlayer();\n    },\n\n    getCount() {\n      return this.store.count;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAMA,SAASA,YAAT,QAA6B,oBAA7B;AACA,SAASC,MAAT,QAAuB,iBAAvB;AACA,eAAe;EACXC,IAAI,EAAE,QADK;EAEXC,KAAK,EAAE,CAAC,UAAD,CAFI;;EAGXC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIL,YAAY,EAA1B;IAEA,OAAO;MAAEK;IAAF,CAAP;EACH,CAPU;;EAQXC,IAAI,GAAG;IACH,OAAO;MACHC,GAAG,EAAE,KAAKC,QADP;MAEHC,UAAU,EAAE,IAFT;MAGHC,OAAO,EAAE,KAAKL,KAAL,CAAWM,SAAX;IAHN,CAAP;EAKH,CAdU;;EAeXC,KAAK,EAAE;IACHF,OAAO,GAAG,CAAE;;EADT,CAfI;EAkBXG,OAAO,EAAE;IACLC,WAAW,GAAG;MACV,MAAMC,IAAI,KAAKV,KAAL,CAAWW,QAAX,KAAwB,CAAxB,IAA6B,CAA7B,GAAiC,GAAjC,GAAuC,GAAjD;;MACA,IAAI,KAAKP,UAAL,KAAoB,IAAxB,EAA8B;QAC1B,KAAKA,UAAL,GAAkBM,CAAlB;QACA,KAAKV,KAAL,CAAWY,YAAX,CAAwB,KAAKV,GAA7B;QACA,KAAKF,KAAL,CAAWa,QAAX;MACJ;;MACA,MAAMC,GAAE,GAAIlB,MAAM,CAAC,KAAKI,KAAL,CAAWe,OAAZ,CAAlB;;MACA,IAAID,GAAG,CAACE,MAAR,EAAgB;QACZC,KAAK,CAAE,GAAEH,GAAG,CAAClB,MAAO,EAAf,CAAL;QACA,KAAKI,KAAL,CAAWkB,SAAX;QACA,KAAKlB,KAAL,CAAWmB,MAAX;MACJ;IACJ;;EAdK,CAlBE;EAkCXC,QAAQ,EAAE;IACNC,SAAS,GAAG;MACR,OAAO,KAAK