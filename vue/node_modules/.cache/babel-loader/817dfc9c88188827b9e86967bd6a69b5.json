{"ast":null,"code":"import { useGameStore } from '../store/gameStore';\nimport { winner } from '../utils/winner';\nexport default {\n  name: \"Square\",\n  props: ['position'],\n\n  setup() {\n    const store = useGameStore();\n    return {\n      store\n    };\n  },\n\n  data() {\n    return {\n      pos: this.position,\n      cell_value: null\n    };\n  },\n\n  watch: {\n    getResult() {\n      this.cell_value = null;\n    }\n\n  },\n  computed: {\n    getPlayer() {\n      return this.store.currentPlayer();\n    },\n\n    getCount() {\n      return this.store.count;\n    },\n\n    getResult() {\n      return this.store.result;\n    }\n\n  },\n  methods: {\n    handleClick() {\n      const a = this.store.getCount() % 2 == 0 ? 'X' : 'O';\n\n      if (this.cell_value === null) {\n        this.cell_value = a;\n        this.store.renewHistory(this.pos);\n        this.store.addCount();\n      }\n\n      const res = winner(this.store.history);\n\n      if (res.result) {\n        alert(`${res.winner}`);\n        this.store.setResult(); //    this.store.$reset()\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAMA,SAASA,YAAT,QAA6B,oBAA7B;AACA,SAASC,MAAT,QAAuB,iBAAvB;AACA,eAAe;EACXC,IAAI,EAAE,QADK;EAEXC,KAAK,EAAE,CAAC,UAAD,CAFI;;EAGXC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIL,YAAY,EAA1B;IACA,OAAO;MAAEK;IAAF,CAAP;EACH,CANU;;EAOXC,IAAI,GAAG;IACH,OAAO;MACHC,GAAG,EAAE,KAAKC,QADP;MAEHC,UAAU,EAAE;IAFT,CAAP;EAIH,CAZU;;EAaXC,KAAK,EAAE;IACHC,SAAS,GAAG;MACR,KAAKF,UAAL,GAAkB,IAAlB;IACJ;;EAHG,CAbI;EAkBXG,QAAQ,EAAE;IACNC,SAAS,GAAG;MACR,OAAO,KAAKR,KAAL,CAAWS,aAAX,EAAP;IACH,CAHK;;IAINC,QAAQ,GAAG;MACP,OAAO,KAAKV,KAAL,CAAWW,KAAlB;IACH,CANK;;IAONL,SAAS,GAAG;MACR,OAAO,KAAKN,KAAL,CAAWY,MAAlB;IACH;;EATK,CAlBC;EA6BXC,OAAO,EAAE;IACLC,WAAW,GAAG;MACV,MAAMC,IAAI,KAAKf,KAAL,CAAWU,QAAX,KAAwB,CAAxB,IAA6B,CAA7B,GAAiC,GAAjC,GAAuC,GAAjD;;MACA,IAAI,KAAKN,UAAL,KAAoB,IAAxB,EAA8B;QAC1B,KAAKA,UAAL,GAAkBW,CAAlB;QACA,KAAKf,KAAL,CAAWgB,YAAX,CAAwB,KAAKd,GAA7B;QACA,KAAKF,KAAL,CAAWiB,QAAX;MACJ;;MACA,MAAMC,GAAE,GAAItB,MAAM,CAAC,KAAKI,KAAL,CAAWmB,OAAZ,CAAlB;;MACA,IAAID,GAAG,CAACN,MAAR,EAAgB;QACZQ,KAAK,CAAE,GAAEF,GAAG,CAACtB,MAAO,EAAf,CAAL;QACA,KAAKI,KAAL,CAAWqB,SAAX,GAFY,CAGZ;MACJ;IACJ;;EAdK;AA7BE,CAAf","names":["useGameStore","winner","name","props","setup","store","data","pos","position","cell_value","watch","getResult","computed","getPlayer","currentPlayer","getCount","count","result","methods","handleClick","a","renewHistory","addCount","res","history","alert","setResult"],"sourceRoot":"","sources":["/Users/strawberrypompey/tacos/frontend/game/frontend/vue/src/components/Square.vue"],"sourcesContent":["<template>\n    <div class=\"square\" @click=\"handleClick\">\n        {{cell_value}}\n    </div>\n</template>\n<script>\nimport { useGameStore } from '../store/gameStore'\nimport { winner } from '../utils/winner'\nexport default {\n    name: \"Square\",\n    props: ['position'],\n    setup() {\n        const store = useGameStore()\n        return { store }\n    },\n    data() {\n        return {\n            pos: this.position,\n            cell_value: null,\n        }\n    },\n    watch: {\n        getResult() {\n            this.cell_value = null\n        }\n    },\n    computed: {\n        getPlayer() {\n            return this.store.currentPlayer()\n        },\n        getCount() {\n            return this.store.count\n        },\n        getResult() {\n            return this.store.result;\n        },\n    },\n    methods: {\n        handleClick() {\n            const a = this.store.getCount() % 2 == 0 ? 'X' : 'O'\n            if (this.cell_value === null) {\n                this.cell_value = a\n                this.store.renewHistory(this.pos)\n                this.store.addCount();\n            }\n            const res = winner(this.store.history)\n            if (res.result) {\n                alert(`${res.winner}`)\n                this.store.setResult();\n                //    this.store.$reset()\n            }\n        }\n    }\n}\n</script>\n<style scoped>\n.square {\n    display: inline-block;\n    height: 100px;\n    width: 100px;\n    border: solid 2px black;\n    font-size: 50px;\n    line-height: 100px;\n    text-align: center;\n}\n\n.square:hover {\n    color: red;\n    border-color: red;\n}\n</style>"]},"metadata":{},"sourceType":"module"}