{"ast":null,"code":"import { useGameStore } from '../store/gameStore';\nexport default {\n  name: \"Square\",\n  props: ['position'],\n\n  setup() {\n    const store = useGameStore();\n    return {\n      store\n    };\n  },\n\n  data() {\n    return {\n      pos: this.position,\n      cell_value: null\n    };\n  },\n\n  methods: {\n    handleClick() {\n      const a = this.store.getCount() % 2 == 0 ? 'X' : 'O';\n\n      if (this.cell_value === null) {\n        this.cell_value = a;\n        this.store.addCount();\n      }\n\n      console.log(a);\n    }\n\n  },\n  computed: {\n    getPlayer() {\n      return this.store.currentPlayer();\n    },\n\n    getCount() {\n      return this.store.count;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAMA,SAASA,YAAT,QAA6B,oBAA7B;AACA,eAAe;EACXC,IAAI,EAAE,QADK;EAEXC,KAAK,EAAE,CAAC,UAAD,CAFI;;EAGXC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIJ,YAAY,EAA1B;IAEA,OAAO;MAAEI;IAAF,CAAP;EACH,CAPU;;EAQXC,IAAI,GAAG;IACH,OAAO;MACHC,GAAG,EAAE,KAAKC,QADP;MAEHC,UAAU,EAAE;IAFT,CAAP;EAIH,CAbU;;EAcXC,OAAO,EAAE;IACLC,WAAW,GAAG;MACV,MAAMC,IAAI,KAAKP,KAAL,CAAWQ,QAAX,KAAwB,CAAxB,IAA6B,CAA7B,GAAiC,GAAjC,GAAuC,GAAjD;;MACA,IAAI,KAAKJ,UAAL,KAAoB,IAAxB,EAA8B;QAC1B,KAAKA,UAAL,GAAkBG,CAAlB;QACA,KAAKP,KAAL,CAAWS,QAAX;MACJ;;MACAC,OAAO,CAACC,GAAR,CAAYJ,CAAZ;IACJ;;EARK,CAdE;EAwBXK,QAAQ,EAAE;IACNC,SAAS,GAAG;MACR,OAAO,KAAKb,KAAL,CAAWc,aAAX,EAAP;IACH,CAHK;;IAINN,QAAQ,GAAG;MACP,OAAO,KAAKR,KAAL,CAAWe,KAAlB;IACJ;;EANM;AAxBC,CAAf","names":["useGameStore","name","props","setup","store","data","pos","position","cell_value","methods","handleClick","a","getCount","addCount","console","log","computed","getPlayer","currentPlayer","count"],"sourceRoot":"","sources":["/Users/strawberrypompey/tacos/frontend/game/frontend/vue/src/components/Square.vue"],"sourcesContent":["<template>\n    <div class=\"square\" @click=\"handleClick\">\n        {{cell_value}}\n    </div>\n</template>\n<script>\nimport { useGameStore } from '../store/gameStore'\nexport default {\n    name: \"Square\",\n   