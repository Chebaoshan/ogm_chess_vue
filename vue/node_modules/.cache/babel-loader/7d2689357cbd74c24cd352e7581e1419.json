{"ast":null,"code":"import { useGameStore } from '../store/gameStore';\nimport { winner } from '../utils/winner';\nexport default {\n  name: \"Square\",\n  props: ['position'],\n\n  setup() {\n    const store = useGameStore();\n    return {\n      store\n    };\n  },\n\n  data() {\n    return {\n      pos: this.position,\n      cell_value: null\n    };\n  },\n\n  methods: {\n    handleClick() {\n      const a = this.store.getCount() % 2 == 0 ? 'X' : 'O';\n\n      if (this.cell_value === null) {\n        this.cell_value = a;\n        this.store.renewHistory(this.pos);\n        this.store.addCount();\n      }\n\n      const res = winner(this.store.history);\n\n      if (res.result) {\n        alert(`winner is ${res.winner}`);\n      }\n    }\n\n  },\n  computed: {\n    getPlayer() {\n      return this.store.currentPlayer();\n    },\n\n    getCount() {\n      return this.store.count;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAMA,SAASA,YAAT,QAA6B,oBAA7B;AACA,SAASC,MAAT,QAAuB,iBAAvB;AACA,eAAe;EACXC,IAAI,EAAE,QADK;EAEXC,KAAK,EAAE,CAAC,UAAD,CAFI;;EAGXC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIL,YAAY,EAA1B;IAEA,OAAO;MAAEK;IAAF,CAAP;EACH,CAPU;;EAQXC,IAAI,GAAG;IACH,OAAO;MACHC,GAAG,EAAE,KAAKC,QADP;MAEHC,UAAU,EAAE;IAFT,CAAP;EAIH,CAbU;;EAcXC,OAAO,EAAE;IACLC,WAAW,GAAG;MACV,MAAMC,IAAI,KAAKP,KAAL,CAAWQ,QAAX,KAAwB,CAAxB,IAA6B,CAA7B,GAAiC,GAAjC,GAAuC,GAAjD;;MACA,IAAI,KAAKJ,UAAL,KAAoB,IAAxB,EAA8B;QAC1B,KAAKA,UAAL,GAAkBG,CAAlB;QACA,KAAKP,KAAL,CAAWS,YAAX,CAAwB,KAAKP,GAA7B;QACA,KAAKF,KAAL,CAAWU,QAAX;MACJ;;MACA,MAAMC,GAAE,GAAIf,MAAM,CAAC,KAAKI,KAAL,CAAWY,OAAZ,CAAlB;;MACA,IAAID,GAAG,CAACE,MAAR,EAAgB;QACZC,KAAK,CAAE,aAAYH,GAAG,CAACf,MAAO,EAAzB,CAAL;MACJ;IACJ;;EAZK,CAdE;EA4BXmB,QAAQ,EAAE;IACNC,SAAS,GAAG;MACR,OAAO,KAAKhB,KAAL,CAAWiB,aAAX,EAAP;IACH,CAHK;;IAINT,QAAQ,GAAG;MACP,OAAO,KAAKR,KAAL,CAAWkB,KAAlB;IACJ;;EANM;AA5BC,CAAf","names":["useGameStore","winner","name","props","setup","store","data","pos","position","cell_value","methods","handleClick","a","getCount","renewHistory","addCount","res","history","result","alert","computed","getPlayer","currentPlayer","count"],"sourceRoot":"","sources":["/Users/strawberrypompey/tacos/frontend/game/frontend/vue/src/components/Square.vue"],"sourcesContent":["<template>\n    <div class=\"square\" @click=\"handleClick\">\n        {{cell_value}}\n    </div>\n</template>\n<script>\nimport { useGameStore } from '../store/gameStore'\nimport { winner } from '../utils/winner'\nexport default {\n    name: \"Square\",\n    props: ['position'],\n    setup() {\n        const store = useGameStore()\n\n        return { store }\n    },\n    data() {\n        return {\n            pos: this.position,\n            cell_value: null\n        }\n    },\n    methods: {\n        handleClick() {\n            const a = this.store.getCount() % 2 == 0 ? 'X' : 'O'\n            if (this.cell_value === null) {\n                this.cell_value = a\n                this.store.renewHistory(this.pos)\n                this.store.addCount();\n            }\n            const res = winner(this.store.history)\n            if (res.result) {\n                alert(`winner is ${res.winner}`)\n            }\n        }\n    },\n    computed: {\n        getPlayer() {\n            return this.store.currentPlayer()\n        },\n        getCount() {\n            return this.store.count\n        }\n    }\n}\n</script>\n<style scoped>\n.square {\n    display: inline-block;\n    height: 100px;\n    width: 100px;\n    border: solid 2px black;\n    font-size: 50px;\n    line-height: 100px;\n    text-align: center;\n}\n\n.square:hover {\n    color: red;\n    border-color: red;\n}\n</style>"]},"metadata":{},"sourceType":"module"}