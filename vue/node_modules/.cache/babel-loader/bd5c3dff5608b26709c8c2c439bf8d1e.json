{"ast":null,"code":"import { useGameStore } from '../store/gameStore';\nimport { winner } from '../utils/winner';\nimport { reactive } from 'vue';\nexport default {\n  name: \"Square\",\n  props: ['position'],\n\n  setup() {\n    const store = useGameStore();\n    const gameres = reactive({\n      store\n    });\n    watch(() => store.$state.result, () => {});\n    return {\n      store,\n      gameres\n    };\n  },\n\n  data() {\n    return {\n      pos: this.position,\n      cell_value: null,\n      refresh: this.gameres\n    };\n  },\n\n  methods: {\n    handleClick() {\n      const a = this.store.getCount() % 2 == 0 ? 'X' : 'O';\n\n      if (this.cell_value === null) {\n        this.cell_value = a;\n        this.store.renewHistory(this.pos);\n        this.store.addCount();\n      }\n\n      const res = winner(this.store.history);\n\n      if (res.result) {\n        alert(`${res.winner}`);\n        console.log(this.refresh);\n        this.store.setResult();\n        console.log(this.refresh);\n        this.store.$reset();\n      }\n    }\n\n  },\n  computed: {\n    getPlayer() {\n      return this.store.currentPlayer();\n    },\n\n    getCount() {\n      return this.store.count;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAMA,SAASA,YAAT,QAA6B,oBAA7B;AACA,SAASC,MAAT,QAAuB,iBAAvB;AACA,SAASC,QAAT,QAAyB,KAAzB;AACA,eAAe;EACXC,IAAI,EAAE,QADK;EAEXC,KAAK,EAAE,CAAC,UAAD,CAFI;;EAGXC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIN,YAAY,EAA1B;IAEA,MAAMO,OAAM,GAAIL,QAAQ,CAAC;MAAEI;IAAF,CAAD,CAAxB;IAEAE,KAAK,CAAC,MAAMF,KAAK,CAACG,MAAN,CAAaC,MAApB,EAA4B,MAAM,CAAG,CAArC,CAAL;IAEA,OAAO;MAAEJ,KAAF;MAASC;IAAT,CAAP;EACH,CAXU;;EAYXI,IAAI,GAAG;IACH,OAAO;MACHC,GAAG,EAAE,KAAKC,QADP;MAEHC,UAAU,EAAE,IAFT;MAGHC,OAAO,EAAE,KAAKR;IAHX,CAAP;EAKH,CAlBU;;EAmBXS,OAAO,EAAE;IACLC,WAAW,GAAG;MACV,MAAMC,IAAI,KAAKZ,KAAL,CAAWa,QAAX,KAAwB,CAAxB,IAA6B,CAA7B,GAAiC,GAAjC,GAAuC,GAAjD;;MACA,IAAI,KAAKL,UAAL,KAAoB,IAAxB,EAA8B;QAC1B,KAAKA,UAAL,GAAkBI,CAAlB;QACA,KAAKZ,KAAL,CAAWc,YAAX,CAAwB,KAAKR,GAA7B;QACA,KAAKN,KAAL,CAAW