{"ast":null,"code":"import { useGameStore } from '../store/gameStore';\nimport { winner } from '../utils/winner';\nexport default {\n  name: \"Square\",\n  props: ['position'],\n\n  setup() {\n    const store = useGameStore();\n    return {\n      store\n    };\n  },\n\n  data() {\n    return {\n      pos: this.position,\n      cell_value: null,\n      refresh: this.gameres\n    };\n  },\n\n  computed: {\n    getPlayer() {\n      return this.store.currentPlayer();\n    },\n\n    getCount() {\n      return this.store.count;\n    }\n\n  },\n  methods: {\n    handleClick() {\n      const a = this.store.getCount() % 2 == 0 ? 'X' : 'O';\n\n      if (this.cell_value === null) {\n        this.cell_value = a;\n        this.store.renewHistory(this.pos);\n        this.store.addCount();\n      }\n\n      const res = winner(this.store.history);\n\n      if (res.result) {\n        alert(`${res.winner}`);\n        console.log(this.refresh);\n        this.store.setResult();\n        console.log(this.refresh);\n        this.store.$reset();\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAMA,SAASA,YAAT,QAA6B,oBAA7B;AACA,SAASC,MAAT,QAAuB,iBAAvB;AACA,eAAe;EACXC,IAAI,EAAE,QADK;EAEXC,KAAK,EAAE,CAAC,UAAD,CAFI;;EAGXC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIL,YAAY,EAA1B;IACA,OAAO;MAAEK;IAAF,CAAP;EACH,CANU;;EAOXC,IAAI,GAAG;IACH,OAAO;MACHC,GAAG,EAAE,KAAKC,QADP;MAEHC,UAAU,EAAE,IAFT;MAGHC,OAAO,EAAE,KAAKC;IAHX,CAAP;EAKH,CAbU;;EAcXC,QAAQ,EAAE;IACNC,SAAS,GAAG;MACR,OAAO,KAAKR,KAAL,CAAWS,aAAX,EAAP;IACH,CAHK;;IAINC,QAAQ,GAAG;MACP,OAAO,KAAKV,KAAL,CAAWW,KAAlB;IACJ;;EANM,CAdC;EAsBXC,OAAO,EAAE;IACLC,WAAW,GAAG;MACV,MAAMC,IAAI,KAAKd,KAAL,CAAWU,QAAX,KAAwB,CAAxB,IAA6B,CAA7B,GAAiC,GAAjC,GAAuC,GAAjD;;MACA,IAAI,KAAKN,UAAL,KAAoB,IAAxB,EAA8B;QAC1B,KAAKA,UAAL,GAAkBU,CAAlB;QACA,KAAKd,KAAL,CAAWe,YAAX,CAAwB,KAAKb,GAA7B;QACA,KAAKF,KAAL,CAAWgB,QAAX;MACJ;;MACA,MAAMC,GAAE,GAAIrB,MAAM,CAAC,KAAKI,KAAL,CAAWkB,OAAZ,CAAlB;;MACA,IAAID,GAAG,CAACE,MAAR,EAAgB;QACZC,KAAK,CAAE,GAAEH,GAAG,CAACrB,MAAO,EAAf,CAAL;QACAyB,OAAO,CAACC,G