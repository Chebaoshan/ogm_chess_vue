{"ast":null,"code":"import { useGameStore } from '../store/gameStore';\nimport { winner } from '../utils/winner';\nexport default {\n  name: \"Square\",\n  props: ['position'],\n\n  setup() {\n    const store = useGameStore();\n    return {\n      store\n    };\n  },\n\n  data() {\n    return {\n      pos: this.position,\n      cell_value: null,\n      refresh: this.store.result\n    };\n  },\n\n  methods: {\n    handleClick() {\n      const a = this.store.getCount() % 2 == 0 ? 'X' : 'O';\n\n      if (this.cell_value === null) {\n        this.cell_value = a;\n        this.store.renewHistory(this.pos);\n        this.store.addCount();\n      }\n\n      const res = winner(this.store.history);\n\n      if (res.result) {\n        alert(`${res.winner}`);\n        console.log(this.store.getResult());\n        this.store.setResult();\n        console.log(this.store.getResult());\n        this.store.$reset();\n      }\n    }\n\n  },\n  computed: {\n    getPlayer() {\n      return this.store.currentPlayer();\n    },\n\n    getCount() {\n      return this.store.count;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAMA,SAASA,YAAT,QAA6B,oBAA7B;AACA,SAASC,MAAT,QAAuB,iBAAvB;AACA,eAAe;EACXC,IAAI,EAAE,QADK;EAEXC,KAAK,EAAE,CAAC,UAAD,CAFI;;EAGXC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIL,YAAY,EAA1B;IAEA,OAAO;MAAEK;IAAF,CAAP;EACH,CAPU;;EAQXC,IAAI,GAAG;IACH,OAAO;MACHC,GAAG,EAAE,KAAKC,QADP;MAEHC,UAAU,EAAE,IAFT;MAGHC,OAAO,EAAE,KAAKL,KAAL,CAAWM;IAHjB,CAAP;EAKH,CAdU;;EAeXC,OAAO,EAAE;IACLC,WAAW,GAAG;MACV,MAAMC,IAAI,KAAKT,KAAL,CAAWU,QAAX,KAAwB,CAAxB,IAA6B,CAA7B,GAAiC,GAAjC,GAAuC,GAAjD;;MACA,IAAI,KAAKN,UAAL,KAAoB,IAAxB,EAA8B;QAC1B,KAAKA,UAAL,GAAkBK,CAAlB;QACA,KAAKT,KAAL,CAAWW,YAAX,CAAwB,KAAKT,GAA7B;QACA,KAAKF,KAAL,CAAWY,QAAX;MACJ;;MACA,MAAMC,GAAE,GAAIjB,MAAM,CAAC,KAAKI,KAAL,CAAWc,OAAZ,CAAlB;;MACA,IAAID,GAAG,CAACP,MAAR,EAAgB;QACZS,KAAK,CAAE,GAAEF,GAAG,CAACjB,MAAO,EAAf,CAAL;QACAoB,OAAO,CAACC,GAAR,CAAY,KAAKjB,KAAL,CAAWkB,SAAX,EAAZ;QACA,KAAKlB,KAAL,CAAWmB,SAAX;QACAH,OAAO,CAACC,GAAR,CAAY,KAAKjB,KAAL,CAAWk