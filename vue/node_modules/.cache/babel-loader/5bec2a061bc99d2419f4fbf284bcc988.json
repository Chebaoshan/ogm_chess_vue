{"ast":null,"code":"import { useGameStore } from '../store/gameStore';\nimport { winner } from '../utils/winner';\nexport default {\n  name: \"Square\",\n  props: ['position'],\n\n  setup() {\n    const store = useGameStore();\n    return {\n      store\n    };\n  },\n\n  data() {\n    return {\n      pos: this.position,\n      cell_value: null,\n      refresh: this.store.getResult()\n    };\n  },\n\n  watch: {\n    refresh() {\n      this.refresh, () => {\n        console.log(1);\n      };\n    }\n\n  },\n  methods: {\n    handleClick() {\n      const a = this.store.getCount() % 2 == 0 ? 'X' : 'O';\n\n      if (this.cell_value === null) {\n        this.cell_value = a;\n        this.store.renewHistory(this.pos);\n        this.store.addCount();\n      }\n\n      const res = winner(this.store.history);\n\n      if (res.result) {\n        alert(`${res.winner}`);\n        this.store.setResult(); // this.store.$reset()\n      }\n    }\n\n  },\n  computed: {\n    getPlayer() {\n      return this.store.currentPlayer();\n    },\n\n    getCount() {\n      return this.store.count;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAMA,SAASA,YAAT,QAA6B,oBAA7B;AACA,SAASC,MAAT,QAAuB,iBAAvB;AACA,eAAe;EACXC,IAAI,EAAE,QADK;EAEXC,KAAK,EAAE,CAAC,UAAD,CAFI;;EAGXC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIL,YAAY,EAA1B;IAEA,OAAO;MAAEK;IAAF,CAAP;EACH,CAPU;;EAQXC,IAAI,GAAG;IACH,OAAO;MACHC,GAAG,EAAE,KAAKC,QADP;MAEHC,UAAU,EAAE,IAFT;MAGHC,OAAO,EAAE,KAAKL,KAAL,CAAWM,SAAX;IAHN,CAAP;EAKH,CAdU;;EAeXC,KAAK,EAAE;IACHF,OAAO,GAAG;MACN,KAAKA,OAAL,EAAc,MAAM;QAAEG,OAAO,CAACC,GAAR,CAAY,CAAZ;MAAe,CAArC;IAEJ;;EAJG,CAfI;EAqBXC,OAAO,EAAE;IACLC,WAAW,GAAG;MACV,MAAMC,IAAI,KAAKZ,KAAL,CAAWa,QAAX,KAAwB,CAAxB,IAA6B,CAA7B,GAAiC,GAAjC,GAAuC,GAAjD;;MACA,IAAI,KAAKT,UAAL,KAAoB,IAAxB,EAA8B;QAC1B,KAAKA,UAAL,GAAkBQ,CAAlB;QACA,KAAKZ,KAAL,CAAWc,YAAX,CAAwB,KAAKZ,GAA7B;QACA,KAAKF,KAAL,CAAWe,QAAX;MACJ;;MACA,MAAMC,GAAE,GAAIpB,MAAM,CAAC,KAAKI,KAAL,CAAWiB,OAAZ,CAAlB;;MACA,IAAID,GAAG,CAACE,MAAR,EAAgB;QACZC,KAAK,CAAE,GAAEH,GAAG,CAACpB,MAAO,EAAf,CAAL;QACA,KAAKI,KAAL,CAAWoB,SAAX,GAFY,CAGZ;MACJ;IACJ;;EAdK,CArBE;EAqCXC,QAAQ,EAAE;IACNC,SAAS,GAAG;MACR,OAAO,KAAKtB,KAAL,CAAWuB,aAAX,EAAP;IACH,CAHK;;IAINV,QAAQ,GAAG;MACP,OAAO,KAAKb,KAAL,CAAWwB,KAAlB;IACJ;;EANM;AArCC,CAAf","names":["useGameStore","winner","name","props","setup","store","data","pos","position","cell_value","refresh","getResult","watch","console","log","methods","handleClick","a","getCount","renewHistory","addCount","res","history","result","alert","setResult","computed","getPlayer","currentPlayer","count"],"sourceRoot":"","sources":["/Users/strawberrypompey/tacos/frontend/game/frontend/vue/src/components/Square.vue"],"sourcesContent":["<template>\n    <div class=\"square\" @click=\"handleClick\">\n        {{cell_value}}\n    </div>\n</template>\n<script>\nimport { useGameStore } from '../store/gameStore'\nimport { winner } from '../utils/winner'\nexport default {\n    name: \"Square\",\n    props: ['position'],\n    setup() {\n        const store = useGameStore()\n\n        return { store }\n    },\n    data() {\n        return {\n            pos: this.position,\n            cell_value: null,\n            refresh: this.store.getResult()\n        }\n    },\n    watch: {\n        refresh() {\n            this.refresh, () => { console.log(1) }\n\n        }\n    },\n    methods: {\n        handleClick() {\n            const a = this.store.getCount() % 2 == 0 ? 'X' : 'O'\n            if (this.cell_value === null) {\n                this.cell_value = a\n                this.store.renewHistory(this.pos)\n                this.store.addCount();\n            }\n            const res = winner(this.store.history)\n            if (res.result) {\n                alert(`${res.winner}`)\n                this.store.setResult();\n                // this.store.$reset()\n            }\n        }\n    },\n    computed: {\n        getPlayer() {\n            return this.store.currentPlayer()\n        },\n        getCount() {\n            return this.store.count\n        }\n    }\n}\n</script>\n<style scoped>\n.square {\n    display: inline-block;\n    height: 100px;\n    width: 100px;\n    border: solid 2px black;\n    font-size: 50px;\n    line-height: 100px;\n    text-align: center;\n}\n\n.square:hover {\n    color: red;\n    border-color: red;\n}\n</style>"]},"metadata":{},"sourceType":"module"}