{"ast":null,"code":"import { useGameStore } from '../store/gameStore';\nimport { winner } from '../utils/winner';\nimport { ref } from 'vue';\nexport default {\n  name: \"Square\",\n  props: ['position'],\n\n  setup() {\n    const store = useGameStore();\n    const gameres = ref(store.$state.result);\n    watch(gameres.value, () => {\n      console.log(1);\n    }, {\n      immediate: true,\n      deep: true\n    });\n    return {\n      store,\n      gameres\n    };\n  },\n\n  data() {\n    return {\n      pos: this.position,\n      cell_value: null,\n      refresh: this.gameres\n    };\n  },\n\n  methods: {\n    handleClick() {\n      const a = this.store.getCount() % 2 == 0 ? 'X' : 'O';\n\n      if (this.cell_value === null) {\n        this.cell_value = a;\n        this.store.renewHistory(this.pos);\n        this.store.addCount();\n      }\n\n      const res = winner(this.store.history);\n\n      if (res.result) {\n        alert(`${res.winner}`);\n        console.log(this.refresh);\n        this.store.setResult();\n        console.log(this.refresh);\n        this.store.$reset();\n      }\n    }\n\n  },\n  computed: {\n    getPlayer() {\n      return this.store.currentPlayer();\n    },\n\n    getCount() {\n      return this.store.count;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAMA,SAASA,YAAT,QAA6B,oBAA7B;AACA,SAASC,MAAT,QAAuB,iBAAvB;AACA,SAASC,GAAT,QAAoB,KAApB;AACA,eAAe;EACXC,IAAI,EAAE,QADK;EAEXC,KAAK,EAAE,CAAC,UAAD,CAFI;;EAGXC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIN,YAAY,EAA1B;IAEA,MAAMO,OAAM,GAAIL,GAAG,CAACI,KAAK,CAACE,MAAN,CAAaC,MAAd,CAAnB;IAEAC,KAAK,CAACH,OAAO,CAACI,KAAT,EAAgB,MAAM;MAAEC,OAAO,CAACC,GAAR,CAAY,CAAZ;IAAgB,CAAxC,EAA0C;MAAEC,SAAS,EAAE,IAAb;MAAmBC,IAAI,EAAE;IAAzB,CAA1C,CAAL;IAEA,OAAO;MAAET,KAAF;MAASC;IAAT,CAAP;EACH,CAXU;;EAYXS,IAAI,GAAG;IACH,OAAO;MACHC,GAAG,EAAE,KAAKC,QADP;MAEHC,UAAU,EAAE,IAFT;MAGHC,OAAO,EAAE,KAAKb;IAHX,CAAP;EAKH,CAlBU;;EAmBXc,OAAO,EAAE;IACLC,WAAW,GAAG;MACV,MAAMC,IAAI,KAAKjB,KAAL,CAAWkB,QAAX,KAAwB,CAAxB,IAA6B,CAA7B,GAAiC,GAAjC,GAAu