             this.store.renewHistory(this.pos)\n                this.store.addCount();\n            }\n            const res = winner(this.store.history)\n            if (res.result) {\n                alert(`${res.winner}`)\n                console.log(this.refresh)\n                this.store.setResult();\n                console.log(this.refresh)\n                this.store.$reset()\n            }\n        }\n    },\n    computed: {\n        getPlayer() {\n            return this.store.currentPlayer()\n        },\n        getCount() {\n            return this.store.count\n        }\n    }\n}\n</script>\n<style scoped>\n.square {\n    display: inline-block;\n    height: 100px;\n    width: 100px;\n    border: solid 2px black;\n    font-size: 50px;\n    line-height: 100px;\n    text-align: center;\n}\n\n.square:hover {\n    color: red;\n    border-color: red;\n}\n</style>"]},"metadata":{},"sourceType":"module"}