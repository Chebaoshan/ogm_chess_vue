{"ast":null,"code":"import { useGameStore } from '../store/gameStore';\nimport { winner } from '../utils/winner';\nexport default {\n  name: \"Square\",\n  props: ['position'],\n\n  setup() {\n    const store = useGameStore();\n    const gameres = ref(this.store.result);\n    return {\n      store\n    };\n  },\n\n  data() {\n    return {\n      pos: this.position,\n      cell_value: null,\n      refresh: this.store.getResult()\n    };\n  },\n\n  watch: {\n    refresh() {\n      this.refresh, () => {\n        console.log(1);\n      };\n    }\n\n  },\n  methods: {\n    handleClick() {\n      const a = this.store.getCount() % 2 == 0 ? 'X' : 'O';\n\n      if (this.cell_value === null) {\n        this.cell_value = a;\n        this.store.renewHistory(this.pos);\n        this.store.addCount();\n      }\n\n      const res = winner(this.store.history);\n\n      if (res.result) {\n        alert(`${res.winner}`);\n        this.store.setResult();\n        this.store.$reset();\n      }\n    }\n\n  },\n  computed: {\n    getPlayer() {\n      return this.store.currentPlayer();\n    },\n\n    getCount() {\n      return this.store.count;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAMA,SAASA,YAAT,QAA6B,oBAA7B;AACA,SAASC,MAAT,QAAuB,iBAAvB;AACA,eAAe;EACXC,IAAI,EAAE,QADK;EAEXC,KAAK,EAAE,CAAC,UAAD,CAFI;;EAGXC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIL,YAAY,EAA1B;IAEA,MAAMM,OAAM,GAAIC,GAAG,CAAC,KAAKF,KAAL,CAAWG,MAAZ,CAAnB;IAEA,OAAO;MAAEH;IAAF,CAAP;EACH,CATU;;EAUXI,IAAI,GAAG;IACH,OAAO;MACHC,GAAG,EAAE,KAAKC,QADP;MAEHC,UAAU,EAAE,IAFT;MAGHC,OAAO,EAAE,KAAKR,KAAL,CAAWS,SAAX;IAHN,CAAP;EAKH,CAhBU;;EAiBXC,KAAK,EAAE;IACHF,OAAO,GAAG;MACN,KAAKA,OAAL,EAAc,MAAM;QAAEG,OAAO,CAACC,GAAR,CAAY,CAAZ;MAAe,CAArC;IAEJ;;EAJG,CAjBI;EAuBXC,OAAO,EAAE;IACLC,WAAW,GAAG;MACV,MAAMC,IAAI,KAAKf,KAAL,CAAWgB,QAAX,KAAwB,CAAxB,IAA6B,CAA7B,GAAiC,GAAjC,GAAuC,GAAjD;;MACA,IAAI,KAAKT,UAAL,KAAoB,IAAxB,EAA8B;QAC1B,KAAKA,UAAL,GAAkBQ,CAAlB;QACA,KAAKf,KAAL,CAAWiB,YAAX,CAAwB,KAAKZ,GAA7B;QACA,KAAKL,KAAL,CAAWkB,QAAX;MACJ;;MACA,MAAMC,GAAE,GAAI